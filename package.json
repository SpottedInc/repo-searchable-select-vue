{
  "name": "repo-searchable-select-vue",
  "version": "0.1.0",
  "description": "An extended HTMLSelectElement with searchable capabilities written in Vue.JS",
  "main": "dist/repo-datetime-vue.umd.js",
  "module": "dist/repo-datetime-vue.esm.js",
  "unpkg": "dist/repo-datetime-vue.min.js",
  "browser": {
    "./sfc": "src/repo-datetime-vue.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "tags": [
    "vue",
    "select",
    "dropdown",
    "search",
    "searchable dropdown",
    "repo",
    "repotenshoku"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "watch": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.2",
    "rollup": "^1.4.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8"
  }
}
